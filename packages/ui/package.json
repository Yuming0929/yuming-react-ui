{
  "name": "@yuming/ui",
  "version": "0.1.0",
  "description": "A modern React UI component library",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": [
    "**/*.scss",
    "**/*.css"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./style": "./src/style/index.ts",
    "./src/*": "./src/*"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "lucide-react": "^0.344.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly --outDir dist",
    "test": "vitest",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "react",
    "ui",
    "component-library",
    "typescript"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  }
}

